{
	"name": "Temp Ordine",
	"module": "CRM",
	"doctype": "DocType",
	"custom": 0,
	"istable": 0,
	"is_tree": 0,
	"editable_grid": 0,
	"track_changes": 0,
	"quick_entry": 0,
	"allow_rename": 0,
	"allow_import": 0,
	"issingle": 0,
	"allow_guest_to_view": 0,
	"title_field": "name",
	"fields": [
		{
			"fieldname": "order_data",
			"label": "Order Data",
			"fieldtype": "JSON",
			"reqd": 1,
			"description": "JSON containing all order information"
		},
		{
			"fieldname": "created_at",
			"label": "Created At",
			"fieldtype": "Int",
			"reqd": 1,
			"default": 0,
			"description": "Unix timestamp when record was created"
		},
		{
			"fieldname": "expires_at",
			"label": "Expires At",
			"fieldtype": "Int",
			"reqd": 1,
			"default": 0,
			"description": "Unix timestamp when record expires (created_at + 300 seconds)"
		},
		{
			"fieldname": "status",
			"label": "Status",
			"fieldtype": "Select",
			"options": "Active\nConsumed\nExpired",
			"default": "Active",
			"reqd": 1,
			"description": "Status of the temporary order"
		}
	],
	"permissions": [
		{
			"role": "System Manager",
			"read": 1,
			"write": 1,
			"create": 1,
			"delete": 1
		},
		{
			"role": "Guest",
			"read": 1,
			"write": 0,
			"create": 0,
			"delete": 0
		}
	],
	"indexes": [
		{
			"name": "idx_expires_at",
			"fields": ["expires_at"]
		},
		{
			"name": "idx_status",
			"fields": ["status"]
		}
	]
}
